{
    "name": "wezom-cms/catalog",
    "description": "Wezom CMS catalog module",
    "type": "library",
    "version": "8.1.0",
    "authors": [
        {
            "name": "Protsiv Andrey",
            "email": "protsiv.a@wezom.com.ua"
        }
    ],
    "require": {
        "ext-json": "*",
        "wezom-cms/core": "^8.1",
        "spatie/laravel-view-models": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "WezomCms\\Catalog\\": "src/",
            "WezomCms\\Catalog\\Database\\Factories\\": "database/factories",
            "WezomCms\\Catalog\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WezomCms\\Catalog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WezomCms\\Catalog\\CatalogServiceProvider"
            ]
        }
    }
}
